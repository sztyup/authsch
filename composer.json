{
  "name": "sztyup/authsch",
  "description": "AuthSCH provider for laravel/socialite",
  "license": "MIT",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "laravel/socialite": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Sztyup\\Authsch\\": "src/Authsch"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\Authsch\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Authsch\\AuthschServiceProvider"
      ]
    }
  }
}
